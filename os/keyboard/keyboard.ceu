output int KEY;

native do
    void UsbInitialise   (void);
    void KeyboardUpdate  (void);
    char KeyboardGetChar (void);
end

_UsbInitialise();
async do
    loop do
        _KeyboardUpdate();
        var char c = _KeyboardGetChar();
        //await 1s;
        //var char c = 'x';
        if c != '\0' then
            emit KEY => c;
        end
    end
end
