#define GPFSEL1 ((uint*)0x20200004)
#define GPSET0  ((uint*)0x2020001C)
#define GPCLR0  ((uint*)0x20200028)

native @pure _ceu_out_log();

var uint ra;
ra = *GPFSEL1;
ra = ra & ~(7<<18);
ra = ra | 1<<18;
*GPFSEL1 = ra;

loop do
    _ceu_out_log(0, "on\n");
    *GPCLR0 = 1<<16;
    await 2s;
    _ceu_out_log(0, "off\n");
    *GPSET0 = 1<<16;
    await 1s;
end
